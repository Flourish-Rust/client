#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jordan/Projects/client/node_modules/.pnpm/@tauri-apps+cli@1.0.0-rc.8/node_modules/@tauri-apps/cli/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/@tauri-apps+cli@1.0.0-rc.8/node_modules/@tauri-apps/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/@tauri-apps+cli@1.0.0-rc.8/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/node_modules:/home/jordan/Projects/client/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/client/node_modules/@tauri-apps/cli/node_modules:/home/jordan/Projects/client/node_modules/@tauri-apps/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jordan/Projects/client/node_modules/.pnpm/@tauri-apps+cli@1.0.0-rc.8/node_modules/@tauri-apps/cli/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/@tauri-apps+cli@1.0.0-rc.8/node_modules/@tauri-apps/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/@tauri-apps+cli@1.0.0-rc.8/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/node_modules:/home/jordan/Projects/client/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/client/node_modules/@tauri-apps/cli/node_modules:/home/jordan/Projects/client/node_modules/@tauri-apps/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@tauri-apps/cli/tauri.js" "$@"
else
  exec node  "$basedir/../@tauri-apps/cli/tauri.js" "$@"
fi
