#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules:/home/jordan/Projects/server/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/sander@0.5.1/node_modules/sander/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/sander@0.5.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/rimraf@2.7.1/node_modules/rimraf/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/rimraf@2.7.1/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules:/home/jordan/Projects/server/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/sander@0.5.1/node_modules/sander/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/sander@0.5.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rimraf@2.7.1/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rimraf@2.7.1/node_modules/rimraf/bin.js" "$@"
fi
