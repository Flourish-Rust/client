#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/typescript@4.6.3/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules:/home/jordan/Projects/server/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/svelte-preprocess@4.10.6_svelte@3.47.0+typescript@4.6.3/node_modules/svelte-preprocess/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/svelte-preprocess@4.10.6_svelte@3.47.0+typescript@4.6.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/typescript@4.6.3/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules:/home/jordan/Projects/server/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/svelte-preprocess@4.10.6_svelte@3.47.0+typescript@4.6.3/node_modules/svelte-preprocess/node_modules:/home/jordan/Projects/server/flourish-rust/node_modules/.pnpm/svelte-preprocess@4.10.6_svelte@3.47.0+typescript@4.6.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsserver" "$@"
fi
