#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jordan/Projects/client/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/typescript@4.6.3/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/node_modules:/home/jordan/Projects/client/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/svelte-check@2.7.0_8ad6e20ce4e2d7da0239e3010622d126/node_modules/svelte-check/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/svelte-check@2.7.0_8ad6e20ce4e2d7da0239e3010622d126/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jordan/Projects/client/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/typescript@4.6.3/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/node_modules:/home/jordan/Projects/client/node_modules:/home/jordan/Projects/node_modules:/home/jordan/node_modules:/home/node_modules:/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/svelte-check@2.7.0_8ad6e20ce4e2d7da0239e3010622d126/node_modules/svelte-check/node_modules:/home/jordan/Projects/client/node_modules/.pnpm/svelte-check@2.7.0_8ad6e20ce4e2d7da0239e3010622d126/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
fi
